  object UniQuery6: TUniQuery
    Connection = FrmMainMenu.UniConnection1
    SQL.Strings = (
      ''
      'DECLARE @DID nvarchar(5) = :G'
      ''
      'UPDATE G'
      'SET G.vol = G.orders * D.fvol * DT.wt, '
      'G.cost = G.vol * D.cost, '
      'G.kcal = G.vol * D.fkcal, '
      'G.osmo = G.vol * D.fosmo, '
      'G.n = G.vol * D.FN,'
      
        'G.vol30 =  CASE WHEN D.type > 1 THEN (G.vol / (DT.totalval - DT.' +
        'lipidvol)) * ((DT.totalval - DT.lipidvol) + 30) '
      
        'ELSE (G.vol / (DT.totalval - DT.lipidvol)) * (DT.totalval - DT.l' +
        'ipidvol) END'
      'FROM TPNtempdatadg AS G INNER JOIN TPNdrug AS D ON G.dg = D.dgid'
      'INNER JOIN TPNtempdata AS DT ON G.ddgid = DT.did '
      'WHERE DT.did = @DID '
      '-- End Query 1 D id have problem'
      ''
      ''
      'DELETE FROM TPNsumlipidvol'
      'WHERE id = @DID'
      ''
      'INSERT INTO TPNsumlipidvol'
      'SELECT D.ddgid AS id, Sum(D.vol) AS SumOfvol '
      'FROM TPNtempdatadg AS D '
      'INNER JOIN TPNdrug AS G '
      'ON D.dg = G.dgid'
      'WHERE G.type = '#39'1'#39
      'GROUP BY D.ddgid'
      'HAVING D.ddgid = @DID '
      '-- End Q 2 '
      ''
      ''
      'DELETE FROM TPNsumothervol'
      'WHERE id = @DID'
      ''
      ''
      'INSERT INTO TPNsumothervol'
      'SELECT D.ddgid AS id , Sum(D.vol) AS SumOfvol '
      'FROM TPNtempdatadg AS D'
      'INNER JOIN TPNdrug AS G '
      'ON D.dg = G.dgid'
      'WHERE G.type > '#39'1'#39
      'GROUP BY D.ddgid'
      'HAVING D.ddgid = @DID '
      '-- End Q 3'
      ''
      'UPDATE D'
      'SET D.lipidvol = L.sumofvol, '
      'D.watervol = D.totalval - L.sumofvol - OT.sumofvol'
      'FROM TPNsumothervol AS OT'
      'INNER JOIN TPNtempdata  AS D'
      'INNER JOIN TPNsumlipidvol  AS L ON D.did = L.id '
      'ON OT.id = D.did'
      'WHERE D.did = @DID '
      '-- End Q 4 check again '
      ''
      'UPDATE G'
      'SET G.osmo = G.osmo /(D.totalval- D.lipidvol)'
      'FROM TPNtempdata AS D '
      'INNER JOIN TPNtempdatadg AS G '
      'ON D.did = G.ddgid '
      'INNER JOIN TPNdrug As DG'
      'ON G.dg = DG.dgid '
      'WHERE DG.type <> '#39'1'#39' '
      'AND D.totalval>0'
      'AND D.did = @DID '
      '-- End Q 5 Not finish'
      ''
      ''
      'DELETE FROM TPNsumosmo_cost'
      'WHERE id = @DID'
      ''
      ' '
      'INSERT INTO TPNsumosmo_cost'
      
        'SELECT DDGID, Sum(G.osmo) AS SumOfosmo, Sum(G.cost) AS SumOfcost' +
        ', Sum(G.kcal) AS SumOfkcal '
      'FROM TPNtempdatadg AS G'
      'GROUP BY G.DDGID'
      'HAVING ddgid = @DID '
      '-- End Q 6 '
      ''
      ''
      'DELETE FROM TPNsumnpc'
      'WHERE id = @DID'
      ''
      'INSERT INTO TPNsumnpc'
      'SELECT G.ddgid, Sum(G.n) AS SumOfn '
      
        'FROM TPNtempdatadg AS G INNER JOIN TPNDrug AS dg ON G.dg  = dg.d' +
        'gid'
      'WHERE dg.type <> '#39'2'#39
      'GROUP BY G.ddgid'
      'HAVING G.ddgid = @DID '
      '-- End Q 7'
      ''
      ''
      'DELETE FROM TPNsumn'
      'WHERE id = @DID'
      ''
      ''
      'INSERT INTO TPNsumn'
      'SELECT g.ddgid, Sum(g.n) AS SumOfn '
      
        'FROM TPNtempdatadg AS g INNER JOIN TPNdrug AS dg ON g.dg = dg.dg' +
        'id'
      'WHERE dg.type = '#39'2'#39
      'GROUP BY g.ddgid'
      'HAVING g.ddgid = @DID '
      '-- End Q 8'
      ''
      'UPDATE P'
      'SET P.SumOfn = P.sumOfn / N.sumofn'
      'FROM TPNsumnpc AS P'
      'INNER JOIN TPNsumn AS  N  ON P.id = N.id '
      'WHERE N.sumofn >0 '
      'AND P.id = @DID '
      '-- End Q 9'
      ''
      ''
      'UPDATE D '
      'SET D.kcal = O.sumofkcal, '
      'D.osmo = O.sumofosmo, '
      'D.cost = O.sumofcost +150, '
      'D.NPC = N.sumofn, '
      'D.mkdate = GETDATE(),'
      'D.mktime = CONVERT (time, GETDATE()), '
      '--D.prepared = '#39#39','
      'D.totalval = D.volkg * D.wt'
      'FROM TPNtempdata AS D '
      'LEFT JOIN TPNsumnpc AS N ON D.did = N.id '
      'LEFT JOIN TPNsumosmo_cost AS O ON D.did = O.id '
      'WHERE D.did = @DID '
      '-- End Qurey 0 is Number 10'
      ''
      ''
      'UPDATE G'
      
        'SET G.note = CASE WHEN  (G.vol*100)/(2*(D.totalval-D.lipidvol))<' +
        '12'
      'THEN '#39'OK '#39' + Left((G.vol*100)/(2*(D.totalval-D.lipidvol)),4)'
      
        'ELSE '#39'too much '#39' + Left((G.vol*100)/(2*(D.totalval-D.lipidvol)),' +
        '4) END'
      'FROM TPNtempdata  AS D INNER JOIN TPNtempdatadg AS G '
      'ON D.did = G.ddgid '
      'WHERE G.dg=1'
      'AND D.did = @DID '
      '-- End Query 11 '
      ''
      '/*'
      'UPDATE G '
      'SET G.note = kval([9],[13],[2],([totvol]-[lipidvol]),[wt])'
      'FROM TPNtempdatadg AS G INNER JOIN TPNtempcrosstab AS T '
      'ON G.id = T.id'
      'WHERE G.dg=13'
      '-- Not final'
      '*/'
      ''
      ''
      'UPDATE G'
      
        'SET G.note = dbo.GetKval(D.DG09,D.DG13,D.DG02,totalval-lipidvol,' +
        'wt)'
      'FROM TPNtempdatadg AS G INNER JOIN'
      '(SELECT *, '
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '001'#39' ) AS DG01, '
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '002'#39' ) AS DG02,'
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '003'#39' ) AS DG03,'
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '004'#39' ) AS DG04, '
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '005'#39' ) AS DG05,'
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '006'#39' ) AS DG06,'
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '007'#39' ) AS DG07, '
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '008'#39' ) AS DG08,'
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '009'#39' ) AS DG09,'
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '010'#39' ) AS DG10, '
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '011'#39' ) AS DG11,'
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '012'#39' ) AS DG12,'
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '013'#39' ) AS DG13, '
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '014'#39' ) AS DG14,'
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '015'#39' ) AS DG15,'
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '016'#39' ) AS DG16, '
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '017'#39' ) AS DG17,'
      
        '(SELECT orders FROM TPNtempdatadg WHERE DDGID = DID and dg = '#39'00' +
        '018'#39' ) AS DG18  '
      'FROM TPNtempdata) AS D'
      'ON G.DDGID = D.DID '
      'WHERE G.dg = '#39'00013'#39
      'AND D.did = @DID')
    Left = 792
    Top = 288
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'G'
      end>
  end
